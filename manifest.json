{
    "manifest_version": 2,
    "name": "My Minutes",
    "description": "The least popular Chrome extension, with atleast 1 user! Blocks urls all over the web.",
    "version": "1.0",
    
    "icons":{
        "16": "images/MyMinutes16.png",
        "48": "images/MyMinutes48.png",
        "128": "images/MyMinutes128.png"  
    },
    
    "browser_action":{
        "default_title": "My Minutes",
        "default_icon": "images/MyMinutes128.png",
        "default_popup": "public_html/popup.html"
    },
    "background":{
        "scripts": ["jquery-3.3.1.js","javaScript/background.js"],
        "persistent": true
    },
    "permissions": [
        "background",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "notifications",
        "activeTab",
        "https://ajax.googleapis.com/",
        "<all_urls>",
        "tabs",
        "app"
    ]
}
